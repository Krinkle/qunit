{
  "index_name": "qunitjs_com",
  "start_urls": [
    { "url": "https://qunitjs.com", "page_rank": 20 },
    { "url": "https://qunitjs.com/blog/", "selectors_key": "blog", "page_rank": 1 }
  ],
  "selectors": {
    "lvl0": "h1",
    "lvl1": "h2:not(.screen-reader-text)",
    "lvl2": "h3:not(.screen-reader-text)",
    "lvl3": "h4:not(.screen-reader-text)",
    "lvl4": "h5:not(.screen-reader-text)",
    "lvl5": "h6:not(.screen-reader-text)",
    "text": ".content p, .content li, .content tr, .content pre"
  },
  "custom_settings": {
    "token_separators": ["_", "-", "."]
  },
  "selectors_exclude": [
    ".posts",
    "aside.sidebar",
    ".toc-wrapper"
  ],
  "scrape_start_urls": false
}
